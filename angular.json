          },
          "configurations": {
            "production": {
              "tsConfig": "projects/material-administration/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/material-administration/src/test.ts",
            "tsConfig": "projects/material-administration/tsconfig.spec.json",
            "karmaConfig": "projects/material-administration/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/material-administration/.eslintrc.js",
            "tsConfig": [
              "projects/material-administration/tsconfig.lib.json",
              "projects/material-administration/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "material-ckeditor": {
      "projectType": "library",
      "root": "projects/material-ckeditor",
      "sourceRoot": "projects/material-ckeditor/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/material-ckeditor/tsconfig.lib.json",
            "project": "projects/material-ckeditor/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/material-ckeditor/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/material-ckeditor/src/test.ts",
            "tsConfig": "projects/material-ckeditor/tsconfig.spec.json",
            "karmaConfig": "projects/material-ckeditor/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/material-ckeditor/.eslintrc.js",
            "tsConfig": [
              "projects/material-ckeditor/tsconfig.lib.json",
              "projects/material-ckeditor/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "material-form": {
      "projectType": "library",
      "root": "projects/material-form",
      "sourceRoot": "projects/material-form/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/material-form/tsconfig.lib.json",
            "project": "projects/material-form/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/material-form/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/material-form/src/test.ts",
            "tsConfig": "projects/material-form/tsconfig.spec.json",
            "karmaConfig": "projects/material-form/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": "projects/material-form/.eslintrc.js",
            "tsConfig": [
              "projects/material-form/tsconfig.lib.json",
              "projects/material-form/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "administration",
  "cli": {
    "analytics": false
  }
}
