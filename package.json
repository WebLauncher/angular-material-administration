{
  "name": "administration",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ngcc && yarn run clean && run-p watch:* serve",
    "build:projects": "ng build material-file-input && ng build material-administration",
    "build:prod": "ng build --prod",
    "serve": "wait-on projects/administration/dist/public-api.d.ts && ng serve",
    "watch:material-file-input": "ng build material-file-input --watch",
    "watch:material-ckeditor": "ng build material-ckeditor --watch",
    "watch:material-form": "wait-on projects/file-input/dist/public-api.d.ts projects/ckeditor/dist/public-api.d.ts && ng build material-form --watch",
    "watch:material-administration": "wait-on projects/form/dist/public-api.d.ts && ng build material-administration --watch",
    "deploy": "ng build --prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint && yarn run lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\" && stylelint \"projects/**/*.scss\"",
    "e2e": "ng e2e",
    "clean": "rimraf projects/**/dist"
  },
  "dependencies": {
    "@angular-material-extensions/password-strength": "~6.0.0",
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "~10.0.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/fire": "~6.0.0",
    "@angular/flex-layout": "~10.0.0-beta.32",
    "@angular/forms": "~10.0.3",
    "@angular/material": "~10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@ckeditor/ckeditor5-angular": "~1.2.3",
    "@ckeditor/ckeditor5-build-classic": "~20.0.0",
    "@firebase/app": "~0.6.1",
    "@firebase/app-types": "~0.6.1",
    "@firebase/auth": "~0.14.6",
    "@firebase/firestore": "~1.14.6",
    "@firebase/util": "~0.2.47",
    "firebase": "~7.13.2",
    "lodash-es": "~4.17.15",
    "ngx-auth-firebaseui": "~4.4.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "~8.2.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1000.1",
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.1",
    "@angular-eslint/builder": "^0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "^0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "^0.0.1-alpha.32",
    "@angular-eslint/schematics": "^0.0.1-alpha.32",
    "@angular-eslint/template-parser": "^0.0.1-alpha.32",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "~3.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "~3.6.0",
    "@typescript-eslint/parser": "~3.6.0",
    "commitizen": "~4.1.2",
    "cz-conventional-changelog": "~3.2.0",
    "eslint": "~7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "~2.21.2",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-jsdoc": "~29.1.0",
    "eslint-plugin-prefer-arrow": "~1.2.1",
    "eslint-plugin-prettier": "~3.1.4",
    "firebase-tools": "~8.5.0",
    "fuzzy": "~0.1.3",
    "husky": "~4.2.5",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lerna": "^3.21.1",
    "lint-staged": "^10.2.11",
    "ng-packagr": "~9.1.5",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.0.5",
    "protractor": "~7.0.0",
    "semantic-release": "~17.0.8",
    "stylelint": "~13.6.1",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "~8.10.2",
    "typescript": "~3.9.6",
    "wait-on": "~5.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged --relative"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "workspaces": [
    "projects/*"
  ],
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix"
    ]
  }
}
