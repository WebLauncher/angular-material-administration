{
  "name": "administration",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ngcc && yarn run clean && run-p watch:* serve json-server",
    "build:projects": "ng build material-file-input && ng build material-administration",
    "build:prod": "ng build --configuration production",
    "serve": "wait-on projects/administration/dist/public-api.d.ts && ng serve",
    "watch:material-file-input": "ng build material-file-input --watch",
    "watch:material-ckeditor": "ng build material-ckeditor --watch",
    "watch:material-form": "wait-on projects/file-input/dist/public-api.d.ts projects/ckeditor/dist/public-api.d.ts && ng build material-form --watch",
    "watch:material-administration": "wait-on projects/form/dist/public-api.d.ts && ng build material-administration --watch",
    "deploy": "ng build --configuration production && firebase deploy",
    "test": "ng test",
    "lint": "ng lint --fix && yarn run lint:styles --fix",
    "lint:styles": "stylelint \"src/**/*.scss\" && stylelint \"projects/**/*.scss\"",
    "e2e": "ng e2e",
    "clean": "rimraf projects/**/dist",
    "clean:node": "rimraf node_modules",
    "json-server": "json-server --watch test/db.json"
  },
  "dependencies": {
    "@angular-material-extensions/password-strength": "~10.0.0",
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/fire": "~6.1.5",
    "@angular/flex-layout": "~12.0.0-beta.35",
    "@angular/forms": "~12.2.17",
    "@angular/material": "~12.2.13",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@ckeditor/ckeditor5-angular": "~2.0.1",
    "@ckeditor/ckeditor5-build-classic": "~23.1.0",
    "@firebase/app": "~0.6.1",
    "@firebase/app-types": "~0.6.1",
    "@firebase/auth": "~0.14.6",
    "@firebase/firestore": "~1.14.6",
    "@firebase/storage": "^0.4.1",
    "@firebase/util": "^1.9.6",
    "firebase": "^8.0.1",
    "lodash-es": "~4.17.15",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "uuid": "~8.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1202.18",
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "~4.15.0",
    "@typescript-eslint/parser": "4.28.2",
    "commitizen": "~4.2.3",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "~7.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jsdoc": "~31.6.1",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~3.3.1",
    "firebase-tools": "~8.4.3",
    "fuzzy": "~0.1.3",
    "husky": "4.3.8",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "0.17.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lerna": "^5.6.2",
    "lint-staged": "^10.5.4",
    "ng-packagr": "~12.2.7",
    "npm-run-all": "~4.1.5",
    "pinst": "^2.1.4",
    "prettier": "~2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "~23.1.1",
    "stylelint": "~16.5.0",
    "stylelint-config-standard": "~36.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-scss": "~6.3.0",
    "ts-node": "~9.0.0",
    "typescript": "~4.3.5",
    "wait-on": "~5.2.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged --relative"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "workspaces": [
    "projects/*"
  ],
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix"
    ]
  }
}
